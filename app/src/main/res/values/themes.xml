<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <!--<style name="Theme.TypeGo" parent="Theme.MaterialComponents.DayNight.DarkActionBar"> -->
    <style name="TypeGo" parent="Theme.AppCompat.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/bg_dark_gray</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/bg_lighter_gray</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/bg_dark_gray</item>
        <item name="android:background">@color/bg_lighter_gray</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.TypeGo.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.TypeGo.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.TypeGo.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>